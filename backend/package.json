{
  "name": "sso-interop-backend",
  "version": "1.0.14",
  "license": "MIT",
  "main": "index.ts",
  "private": true,
  "scripts": {
    "build": "tsc && tsup",
    "dev": "node --import=tsx --watch src/index.ts",
    "start": "node dist/index.js"
  },
  "dependencies": {
    "@asteasolutions/zod-to-openapi": "7.3.4",
    "@matterlabs/zksync-js": "^0.0.6",
    "cors": "2.8.5",
    "dotenv": "17.2.1",
    "ethers": "^6.16.0",
    "express": "5.1.0",
    "express-rate-limit": "8.0.1",
    "helmet": "8.1.0",
    "http-status-codes": "2.3.0",
    "pino": "9.9.0",
    "pino-http": "10.5.0",
    "sso-legacy": "npm:zksync-sso",
    "viem": "^2.44.4",
    "zksync-sso": "1.0.0-beta.4",
    "zod": "3.25.76"
  },
  "devDependencies": {
    "@types/cors": "2.8.19",
    "@types/express": "5.0.3",
    "@types/supertest": "6.0.3",
    "@types/swagger-ui-express": "4.1.8",
    "@vitest/coverage-v8": "3.2.4",
    "pino-pretty": "13.1.1",
    "supertest": "7.1.4",
    "tsup": "8.5.0",
    "tsx": "4.20.4",
    "typescript": "5.9.2"
  },
  "tsup": {
    "entry": ["src/index.ts"],
    "outDir": "dist",
    "format": ["esm", "cjs"],
    "target": "es2020",
    "sourcemap": true,
    "clean": true,
    "dts": true,
    "splitting": false,
    "skipNodeModulesBundle": true
  }
}
